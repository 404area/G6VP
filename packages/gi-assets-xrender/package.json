{
  "name": "@alipay/gi-assets-xrender",
  "version": "1.0.0",
  "description": "基于 XRender 的 GraphInsight 资产包",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "remove:antd": "rm -rf ./node_modules/antd",
    "start": "npm run clean && father build --watch",
    "build": "NODE_OPTIONS=--max_old_space_size=2048 npm run clean && father build",
    "build:umd": "node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js  --mode production  -c ./webpack.config.js ",
    "test": "jest",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "clean": "rimraf es esm lib dist"
  },
  "dependencies": {
    "@alipay/graphinsight": "*",
    "form-render": "^1.9.5",
    "@alipay/gi-assets-basic": "*"
  },
  "peerDependencies": {
    "react": "17.x",
    "@antv/graphin": "2.x",
    "antd": "^4.18.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "father": "^2.30.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "publishConfig": {
    "registry": "https://registry.antfin-inc.com"
  }
}
