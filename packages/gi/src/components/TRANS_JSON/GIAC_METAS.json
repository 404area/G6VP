{
  "GI_CONTAINER_INDEX": {
    "title": "排序位置",
    "type": "string",
    "x-component": "Stepper",
    "x-component-props": {
      "step": 1,
      "min": 0,
      "max": 15
    },
    "default": 2
  },
  "GIAC": {
    "type": "void",
    "x-decorator": "FormItem",
    "x-component": "FormCollapse",
    "properties": {
      "GIAC": {
        "type": "object",
        "x-component": "FormCollapse.CollapsePanel",
        "x-component-props": {
          "header": "容器配置"
        },
        "properties": {
          "visible": {
            "title": "默认显示",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": false
          },
          "disabled": {
            "title": "功能禁用",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": false
          },
          "isShowTitle": {
            "title": "显示名称",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": true
          },
          "title": {
            "title": "填写名称",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowTitle",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "时序分析"
          },
          "isShowIcon": {
            "title": "显示图标",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": true
          },
          "icon": {
            "title": "选择图标",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowIcon",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "icon-star"
          },
          "isShowTooltip": {
            "title": "提示框",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": true
          },
          "tooltip": {
            "title": "填写提示框内容",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowTooltip",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": ""
          },
          "tooltipColor": {
            "title": "提示颜色",
            "type": "string",
            "x-component": "Fill",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowTooltip",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "#3056e3"
          },
          "tooltipPlacement": {
            "title": "提示方位",
            "type": "string",
            "x-component": "Select",
            "x-component-props": {
              "options": [
                {
                  "value": "top",
                  "label": "上方"
                },
                {
                  "value": "left",
                  "label": "左方"
                },
                {
                  "value": "right",
                  "label": "右方"
                },
                {
                  "value": "bottom",
                  "label": "下方"
                }
              ],
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowTooltip",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "top"
          },
          "hasDivider": {
            "title": "分隔符",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": false
          },
          "height": {
            "title": "单元高度",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {},
            "default": "60px"
          },
          "isVertical": {
            "title": "垂直排列",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowIcon",
                    "$eq",
                    true
                  ],
                  [
                    ".isShowTitle",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": true
          }
        }
      }
    }
  }
}