{
  "GI_CONTAINER_INDEX": {
    "title": "排序位置",
    "type": "string",
    "x-component": "Stepper",
    "x-component-props": {
      "step": 1,
      "min": 0,
      "max": 15
    },
    "default": 2
  },
  "GIAC_CONTENT": {
    "type": "void",
    "x-decorator": "FormItem",
    "x-component": "FormCollapse",
    "properties": {
      "GIAC_CONTENT": {
        "type": "object",
        "x-component": "FormCollapse.CollapsePanel",
        "x-component-props": {
          "header": "容器配置"
        },
        "properties": {
          "visible": {
            "title": "默认显示",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": false
          },
          "disabled": {
            "title": "功能禁用",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": false
          },
          "isShowTitle": {
            "title": "显示名称",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": true
          },
          "title": {
            "title": "填写名称",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowTitle",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "时序分析"
          },
          "isShowIcon": {
            "title": "显示图标",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": true
          },
          "icon": {
            "title": "选择图标",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowIcon",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "icon-star"
          },
          "isShowTooltip": {
            "title": "提示框",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": true
          },
          "tooltipColor": {
            "title": "提示颜色",
            "type": "string",
            "x-component": "Fill",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowTooltip",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "#3056e3"
          },
          "tooltipPlacement": {
            "title": "提示方位",
            "type": "string",
            "x-component": "Select",
            "x-component-props": {
              "options": [
                {
                  "value": "top",
                  "label": "上方"
                },
                {
                  "value": "left",
                  "label": "左方"
                },
                {
                  "value": "right",
                  "label": "右方"
                },
                {
                  "value": "bottom",
                  "label": "下方"
                }
              ],
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowTooltip",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": "right"
          },
          "hasDivider": {
            "title": "分隔符",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": false
          },
          "height": {
            "title": "单元高度",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {},
            "default": "60px"
          },
          "isVertical": {
            "title": "垂直排列",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {
              "showInPanel": {
                "conditions": [
                  [
                    ".isShowIcon",
                    "$eq",
                    true
                  ],
                  [
                    ".isShowTitle",
                    "$eq",
                    true
                  ]
                ]
              }
            },
            "default": true
          },
          "containerType": {
            "title": "容器类型",
            "type": "string",
            "x-component": "Radio",
            "x-component-props": {
              "options": [
                {
                  "label": "抽屉",
                  "value": "drawer"
                },
                {
                  "label": "弹窗",
                  "value": "modal"
                },
                {
                  "label": "普通DIV",
                  "value": "div"
                }
              ]
            },
            "default": "drawer"
          },
          "containerPlacement": {
            "title": "容器位置",
            "type": "string",
            "x-component": "Select",
            "x-component-props": {
              "options": [
                {
                  "value": "LT",
                  "label": "左上 / top"
                },
                {
                  "value": "LB",
                  "label": "左下 / left"
                },
                {
                  "value": "RT",
                  "label": "右上 / right"
                },
                {
                  "value": "RB",
                  "label": "右下 / bottom"
                }
              ]
            },
            "default": "RT"
          },
          "offset": {
            "title": "偏移距离",
            "type": "string",
            "x-component": "Offset",
            "x-component-props": {
              "min": 0,
              "max": 400
            },
            "default": [
              10,
              60
            ]
          },
          "containerWidth": {
            "title": "容器宽度",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {},
            "default": "400px"
          },
          "containerHeight": {
            "title": "容器宽度",
            "type": "string",
            "x-component": "Text",
            "x-component-props": {},
            "default": "calc(100vh - 120px)"
          },
          "contaienrMask": {
            "title": "容器遮罩",
            "type": "string",
            "x-component": "Switch",
            "x-component-props": {},
            "default": false
          }
        }
      }
    }
  }
}