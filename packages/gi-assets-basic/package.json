{
  "name": "@alipay/gi-assets-basic",
  "version": "2.20.1",
  "description": "GraphInsight 基础资产包",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:es & npm run build:umd",
    "build:es": "father build",
    "build:umd": "webpack  --mode production  -c ../../webpack.config.js --env path=/packages/gi-assets-basic",
    "clean": "rimraf es esm lib dist",
    "docs": "dumi dev",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "remove:antd": "rm -rf ./node_modules/antd",
    "start": "npm run clean && father build --watch",
    "test": "jest",
    "build:umd:analysis": "webpack  --mode production  -c ../../webpack.config.js --env path=/packages/gi-assets-basic analysis=true",
    "tnpm:publish": "npm run build && node ../../scripts/publish_backup.js && tnpm publish && node ../../scripts/publish_recover.js"
  },
  "dependencies": {
    "@alipay/gi-common-components": "*",
    "@alipay/graphinsight": "*",
    "@ant-design/icons": "latest",
    "@antv/g2plot": "^2.4.20",
    "@antv/graphin-icons": "^1.0.0",
    "@antv/util": "^3.2.5",
    "d3-scale": "^4.0.0",
    "deepmerge": "^4.2.2",
    "immer": "^9.0.15",
    "json2csv": "^5.0.7",
    "nanoid": "^4.0.0",
    "use-immer": "^0.7.0"
  },
  "peerDependencies": {
    "@antv/graphin": "2.x",
    "react": "17.x",
    "react-dom": "17.x",
    "antd": "latest"
  },
  "publishConfig": {
    "registry": "https://registry.antfin-inc.com"
  }
}