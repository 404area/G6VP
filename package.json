{
  "dependencies": {
    "@ali/datav-gui-color-scale": "^1.0.9",
    "@ali/datav-gui-marker-scale": "^1.1.0",
    "@ali/datav-gui-size-scale": "^1.0.5",
    "@ali/react-datav-gui": "^4.1.7",
    "@ali/react-datav-gui-utils": "^0.1.18",
    "@ant-design/charts": "^1.2.11",
    "@ant-design/icons": "^4.6.2",
    "@antv/graphin": "latest",
    "@antv/s2-react": "^1.8.0",
    "antd": "latest",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "devDependencies": {
    "@types/react": "17.x",
    "@types/react-dom": "17.x",
    "@umijs/plugin-antd": "^0.11.0",
    "@umijs/plugin-layout": "^0.17.2",
    "@umijs/plugin-locale": "^0.14.2",
    "@umijs/plugin-test": "^1.0.2",
    "ajv": "^8.10.0",
    "css-loader": "^6.6.0",
    "husky": "^6.0.0",
    "lerna": "^4.0.0",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "monaco-editor": "^0.25.2",
    "source-map-loader": "^3.0.1",
    "ts-loader": "^9.2.6",
    "umi": "^3.5.18",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "core": "cd packages/gi && npm run start",
    "assets": "cd packages/gi-assets-basic && npm run start",
    "assets:advanced": "cd packages/gi-assets && npm run start",
    "common:components": "cd packages/gi-common-components && npm run start",
    "assets:testing": "cd packages/gi-assets-testing && npm run start",
    "assets:docs": "cd packages/gi-assets-basic && npm run docs",
    "common:components:docs": "cd packages/gi-common-components && npm run docs",
    "site": "cd packages/gi-site && NODE_OPTIONS=--max_old_space_size=2048 npm run start",
    "bootstrap": "lerna link && lerna bootstrap  --hoist --force-local ",
    "build": "npm run bootstrap && npm run build:core && npm run build:assets && npm run build:site && npm run move:dist",
    "build:core": "cd packages/gi && NODE_OPTIONS=--max_old_space_size=2048 npm run build",
    "build:assets": "cd packages/gi-assets-basic && NODE_OPTIONS=--max_old_space_size=2048 npm run build",
    "build:site": "cd packages/gi-site && NODE_OPTIONS=--max_old_space_size=2048 npm run build",
    "build:basicAssets": "cd packages/gi-assets-basic && NODE_OPTIONS=--max_old_space_size=2048 npm run build",
    "build:testing": "cd packages/gi-assets-testing && NODE_OPTIONS=--max_old_space_size=2048 npm run build",
    "move:dist": "node ./deploy.js",
    "list": "lerna list",
    "link": "lerna link --force-local",
    "clean": "lerna clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
